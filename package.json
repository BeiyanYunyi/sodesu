{
  "name": "sodesu-comment",
  "version": "0.0.1-alpha7",
  "description": "Comment system with SolidJS",
  "packageManager": "pnpm@7.26.0",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "rimraf dist && pnpm build:js && pnpm build:style",
    "build:js": "tsc && rollup -c rollup.config.ts --configPlugin typescript",
    "build:style": "esr scripts/build.ts"
  },
  "main": "./dist/sodesu.solid.module.js",
  "module": "./dist/sodesu.solid.module.js",
  "browser": "./dist/sodesu.solid.module.js",
  "types": "./dist/sodesu.d.ts",
  "keywords": [
    "valine",
    "waline",
    "comment",
    "blog",
    "solid"
  ],
  "repository": {
    "url": "https://github.com/lixiang810/sodesu"
  },
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": "./dist/sodesu.solid.module.js",
      "types": "./dist/sodesu.d.ts",
      "default": "./dist/sodesu.solid.module.js"
    },
    "./sodesu.css": "./dist/sodesu.css",
    "./aio": {
      "import": "./dist/sodesu.aio.module.js",
      "types": "./dist/sodesu.d.ts",
      "require": "./dist/sodesu.aio.umd.js",
      "default": "./dist/sodesu.aio.umd.js"
    },
    "./dist/aio": {
      "import": "./dist/sodesu.aio.module.js",
      "types": "./dist/sodesu.d.ts",
      "require": "./dist/sodesu.aio.umd.js",
      "default": "./dist/sodesu.aio.umd.js"
    },
    "./preset": {
      "import": "./dist/preset.mjs",
      "types": "./dist/preset.d.ts",
      "require": "./dist/preset.cjs",
      "default": "./dist/preset.cjs"
    },
    "./dist/preset": {
      "import": "./dist/preset.mjs",
      "types": "./dist/preset.d.ts",
      "require": "./dist/preset.cjs",
      "default": "./dist/preset.cjs"
    },
    "./component": {
      "import": "./dist/component.js",
      "types": "./dist/component.d.ts",
      "default": "./dist/component.js"
    },
    "./dist/component": {
      "import": "./dist/component.js",
      "types": "./dist/component.d.ts",
      "default": "./dist/component.js"
    },
    "./dist/*": "./dist/*",
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "license": "AGPL-3.0-or-later",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@giphy/js-types": "^4.4.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/babel__core": "^7.20.0",
    "@types/marked": "^4.0.8",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@unocss/core": "^0.49.0",
    "esbuild": "^0.17.4",
    "esbuild-runner": "^2.2.2",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-solid": "^0.9.3",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "rollup": "^3.11.0",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-summary": "^2.0.0",
    "typescript": "^4.9.4",
    "unocss": "^0.49.0",
    "vite": "^4.0.4",
    "vite-plugin-solid": "^2.5.0"
  },
  "dependencies": {
    "@solid-primitives/date": "^2.0.6",
    "@solid-primitives/storage": "^1.3.4",
    "@waline/client": "^2.14.7",
    "solid-js": "^1.6.9"
  }
}
